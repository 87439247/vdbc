<dialect extends="generic" name="H2">
    <example-urls>
        <url>jdbc:h2:tcp://localhost/~/sample</url>
        <url>jdbc:h2:ssl://localhost:8085/~/sample</url>
        <url>jdbc:h2:file:/data/sample</url>
        <url>jdbc:h2:mem:test</url>
    </example-urls>
    <drivers>
        <driver>org.h2.Driver</driver>
    </drivers>
    <features>
        <feature id="select.all.from.table.limit.offset" expression="select * from @{tableName} limit @{limit} offset @{offset}"/>
    </features>
</dialect>